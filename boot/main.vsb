; [version 13]

DEF main
    PRINT "[OK] Version is supported"
    IF NOT $_graphics
        PRINT "[HLT] No graphics module found!"
        HLT
    ENDIF
    PRINT "[OK] Graphics module found"
    PRINT "Switching to kernel"
    CALL "kernel"
    CMPR "NE" $_ret "0"
    IF $_cmpr
        PRINT "[FATAL] Kernel exited with code " $_ret
    ELSE
        PRINT "[OK] Kernel exited with code 0"
    ENDIF
    HLT
ENDDEF main
